{
    "Configuration":{
        "Root": {
            "Pattern": "./configurations",
            "Wildcard": "*"
        },
        "Domain": {
            "Pattern": "/*"
        },
        "Implementation": {
            "Pattern": "/*/impl"
        },
        "Naming": {
            "Patterns": {
                "Domain": "/*/naming.json",
                "General": "/naming.json"
            }
        },
        "Package": {
            "Pattern": "/*/packages/*.json"
        },
        "Tool": {
            "Patterns": {
                "Configuration": "/*/config/*.json",
                "Actual": "/*/impl/*/",
                "Usage": "/*/impl/*/usage.json"
            }
        }
    }
}